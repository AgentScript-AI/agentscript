{
    "name": "chorus",
    "version": "1.0.0",
    "repository": "https://github.com/kedrzu/chorus.git",
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "private": true,
    "scripts": {
        "monorepo": "node ./nzyme/packages/project-tools/dist/monorepo.js",
        "watch": "yarn && yarn monorepo && turbo run watch",
        "dev": "yarn && yarn monorepo && turbo run dev",
        "clean": "tsc --build --clean",
        "build": "tsc --build",
        "start": "yarn --cwd examples/drizzle dev",
        "ngrok": "ngrok http 3001"
    },
    "devDependencies": {
        "@chorus/cli": "*",
        "@types/source-map-support": "^0.5.10",
        "lerna": "^8.1.8",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "turbo": "^2.2.2",
        "typescript": "^5.7.2"
    },
    "workspaces": [
        "integrations/*",
        "packages/*",
        "nzyme/packages/*"
    ],
    "packageManager": "yarn@4.1.0",
    "dependencies": {
        "source-map-support": "^0.5.21",
        "supabase": "1.226.3"
    }
}
